{
  "expo": {
    "name": "pcm-react-native",
    "slug": "pcm-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/pcm-icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.lrmsoft.pcmapp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA8p_w-WvV2nI0l3l5FNMdoh0kLQHvxru0"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_NETWORK_STATE",
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
